{
  "name": "framepay-docs",
  "version": "1.0.0",
  "description": "Official documentation for Rebilly FramePay",
  "private": true,
  "scripts": {
    "base-docs-build": "vuepress build docs",
    "base-docs-babel-examples": "babel docs/.vuepress/dist/examples --out-dir docs/.vuepress/dist/examples --source-maps",
    "base-docs-babel": "run-p base-docs-babel-examples",
    "docs:dev": "vuepress dev docs",
    "docs:build": "run-s base-docs-build base-docs-babel",
    "docs:serve": "browser-sync start --config ./bs-config.js"
  },
  "keywords": [
    "rebilly",
    "framepay",
    "checkout",
    "PCI Compliance",
    "billing",
    "payment",
    "bank account",
    "checkout process"
  ],
  "main": "index.js",
  "repository": "https://github.com/Rebilly/framepay-docs.git",
  "author": "andrei.moldoveanu@rebilly.com",
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://raw.githubusercontent.com/Rebilly/framepay-docs/master/LICENSE"
    }
  ],
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@vuepress/shared-utils": "^1.0.3",
    "browser-sync": "^2.26.7",
    "browsersync": "^0.0.1-security",
    "dotenv": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "replace-in-files": "^2.0.3",
    "vuepress": "^1.0.3",
    "vuepress-plugin-element-tabs": "^0.1.8"
  }
}
